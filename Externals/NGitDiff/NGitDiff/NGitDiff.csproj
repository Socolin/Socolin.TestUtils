<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>NGitDiff</PackageId>
        <Title>NGitDiff</Title>
        <AssemblyTitle>NGitDiff</AssemblyTitle>
        <Description>Diff algorithm from the NGit machine translation of the JGit library to C#"</Description>
        <RootNamespace>NGit</RootNamespace>
        <Copyright>Copyright owners detailed at http://eclipse.org/jgit in the JGit IP log</Copyright>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>
</Project>
